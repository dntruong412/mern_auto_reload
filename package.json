{
    "name": "auto_reload",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "db:start": "docker-compose down --remove-orphans && docker-compose up",
        "server:start": "nodemon server.js",
        "test:db": "nodemon test_db.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "nodemonConfig": {
        "ignore": [
            ".git",
            "node_modules/**/node_modules"
        ],
        "watch": [
            "server.js",
            "test_db.js",
            "src"
        ],
        "verbose": true,
        "ext": "js json"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "mongodb": "^3.3.4",
        "mongoose": "^5.7.12",
        "morgan": "^1.9.1",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "nodemon": "^1.19.4",
        "reload": "^3.0.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0"
    }
}
